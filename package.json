{
  "name": "trending-days-of-code",
  "description": "Mobile-first web application to publicly track trending 'days of code' challenges",
  "author": {
    "name": "Jacob Ward <jacob@riino.co.uk>",
    "url": "http://github.com/jacobwarduk"
  },
  "homepage": "https://github.com/jacobwarduk/trending-days-of-code",
  "version": "0.0.1",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jacobwarduk/trending-days-of-code/blob/master/LICENSE.md"
    }
  ],
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/jacobwarduk/trending-days-of-code/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobwarduk/trending-days-of-code.git"
  },
  "scripts": {
    "lint": "xo --fix",
    "build": "npm run lint && npm run test && webpack",
    "watch": "webpack --watch",
    "test": "jest",
    "dev": "webpack-dev-server"
  },
  "private": true,
  "dependencies": {
    "axios": "0.16.1",
    "babel-plugin-dynamic-import-webpack": "1.0.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-register": "6.24.1",
    "base-64": "^0.1.0",
    "compression": "1.6.2",
    "cors": "2.8.3",
    "lodash": "4.17.4",
    "moment": "^2.18.1",
    "preact": "8.1.0",
    "preact-compat": "3.16.0",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-addons-perf": "15.4.2",
    "react-bootstrap": "^0.31.1",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "styled-components": "2.0.0",
    "webpack": "2.6.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-preset-env": "1.5.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "3.19.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-config-xo-react": "^0.12.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "jest": "^20.0.4",
    "jest-serializer-enzyme": "^1.0.0",
    "moxios": "0.4.0",
    "prop-types": "^15.5.10",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "15.5.4",
    "webpack-dev-server": "2.4.5",
    "xo": "^0.18.2",
    "xo-loader": "^0.8.0"
  },
  "xo": {
    "extends": "xo-react",
    "rules": {
      "camelcase": 0,
      "max-nested-callbacks": 0,
      "unicorn/filename-case": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-handler-names": 0,
      "react/jsx-curly-spacing": [
        "error",
        "always"
      ],
      "import/no-unresolved": 0,
      "react/jsx-tag-spacing": [
        1,
        {
          "beforeSelfClosing": "always",
          "afterOpening": "never"
        }
      ],
      "react/jsx-key": 0
    },
    "globals": [
      "document",
      "describe",
      "it",
      "expect",
      "beforeEach",
      "afterEach"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-enzyme"
    ]
  }
}
